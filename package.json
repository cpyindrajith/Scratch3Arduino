{
  "name": "WiinBlock-desktop",
  "productName": "WiinBlock",
  "description": "WiinBlock as a self-contained desktop application",
  "author": "WiinBlock Team",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "electron-builder install-app-deps && npm run cpdevice && npm run cpext && npm run cptools",
    "cpdevice": "cpy ./ ../../../devices --parents --cwd=./node_modules/openblock-device/src",
    "cpext": "cpy ./ ../../../extensions --parents --cwd=./node_modules/openblock-extension/src",
    "cptools": "cpy ./ ../../../tools --parents --cwd=./node_modules/openblock-link/tools",
    "clean": "rimraf ./dist ./statics ./extensions ./tools ./translations",
    "start": "mkdirp ./dist && electron-webpack dev --bail --display-error-details --env.minify=false --no-progress",
    "compile": "mkdirp ./dist && electron-webpack --bail --display-error-details --env.minify=false --no-progress",
    "fetch": "rimraf ./statics && mkdirp ./statics && git clone https://github.com/openblockcc/openblock-asset.git statics",
    "build": "npm run build:dev",
    "build:dev": "npm run compile && npm run doBuild -- --mode=dev",
    "build:dir": "npm run compile && npm run doBuild -- --mode=dir",
    "build:dist": "npm run compile && npm run doBuild -- --mode=dist",
    "doBuild": "node ./scripts/electron-builder-wrapper.js",
    "dist": "npm run clean && npm run compile && npm run fetch && npm run cpdevice && npm run cpext && npm run cptools && npm run doBuild -- --mode=dist",
    "distDev": "npm run clean && npm run compile && npm run fetch && npm run cpdevice && npm run cpext && npm run cptools && npm run doBuild -- --mode=dev",
    "test": "npm run test:lint",
    "test:lint": "eslint --cache --color --ext .jsx,.js ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/openblockcc/openblock-desktop.git"
  },
  "dependencies": {
    "@abandonware/noble": "^1.9.2-14",
    "compare-versions": "^3.6.0",
    "del": "^6.0.0",
    "openblock-device": "0.1.0-prerelease.20210328115506",
    "openblock-extension": "0.1.0-prerelease.20210329032033",
    "openblock-link": "0.1.0-prerelease.20210326122102",
    "patch-package": "^6.4.7",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.13.0",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "async": "^3.2.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-react-intl": "^7.9.4",
    "copy-webpack-plugin": "^5.1.2",
    "cpy-cli": "^3.1.1",
    "electron": "^8.5.5",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^3.2.0",
    "electron-notarize": "^0.3.0",
    "electron-store": "^5.2.0",
    "electron-webpack": "^2.8.2",
    "eslint": "^7.24.0",
    "eslint-config-scratch": "^6.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.2",
    "fs-extra": "^9.1.0",
    "intl": "1.2.5",
    "lodash.bindall": "^4.4.0",
    "lodash.defaultsdeep": "^4.6.1",
    "minilog": "^3.1.0",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "nets": "^3.2.0",
    "openblock-gui": "github:openblockcc/openblock-gui#openblock-desktop-v2.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-intl": "2.9.0",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "uuid": "^8.3.2",
    "webpack": "^4.46.0"
  },
  "resolutions": {
    "upath": "^1.0.5"
  }
}
